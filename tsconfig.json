{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "es2021"
    ],
    "jsx": "react-native",
    "module": "commonjs",
    "moduleResolution": "node",
    "baseUrl": "./src",
    "project": "./",
    "resolveJsonModule": true,
    "allowJs": true,
    "noEmit": true,
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedIndexedAccess": true,
    "allowUnusedLabels": false,
    "allowUnreachableCode": false,
    "skipLibCheck": true,
    "paths": {
      "components": [
        "components"
      ],
      "components/*": [
        "components/*"
      ],
      "models": [
        "models"
      ],
      "models/*": [
        "models/*"
      ],
      "navigation": [
        "navigation"
      ],
      "navigation/*": [
        "navigation/*"
      ],
      "screens": [
        "screens"
      ],
      "screens/*": [
        "screens/*"
      ],
      "services": [
        "services"
      ],
      "services/*": [
        "services/*"
      ],
      "stores": [
        "stores"
      ],
      "stores/*": [
        "stores/*"
      ],
    }
  },
  "noImplicitAny": true,
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}